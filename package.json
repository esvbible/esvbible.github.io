{
  "name": "esvbible",
  "version": "0.1.0",
  "description": "Offline, mobile ESV Bible",
  "author": "Aaron Stacy",
  "license": "MIT",
  "dependencies": {
    "flow-bin": "^0.54.0",
    "history": "^4.7.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13",
    "redux": "^3.7.2",
    "web-animations-js": "^2.3.1"
  },
  "devDependencies": {
    "flow-watch": "^1.1.1",
    "sw-precache": "^5.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && sw-precache --config=config/sw-precache-config.js && cat build/static/js/0.*.chunk.js | head -1 | sed 's/^[^{]*{[^{]*{//' | sed 's/}});$//' >> build/service-worker.js",
    "typecheck": "[ $CI = true ] && flow || flow-watch",
    "test": "export TEST=true && react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esvbible/esvbible.github.io.git"
  },
  "keywords": [
    "bible",
    "esv",
    "pwa"
  ],
  "bugs": {
    "url": "https://github.com/esvbible/esvbible.github.io/issues"
  },
  "homepage": "https://github.com/esvbible/esvbible.github.io#readme",
  "proxy": "http://www.esvapi.org"
}
